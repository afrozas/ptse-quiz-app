
<h1>Edit question</h1>


<%= form_with(model: @question) do |form| %>

    <p>
    <%= form.label :description %><br>
    <%= form.text_field :description %>
    </p>

    <p>
    <%= form.label :text %><br>
    <%= form.text_area :text %>
    </p>

    <p>
    <%= form.submit %>
    </p>
 <% end %>
 
<%= link_to 'Back', questions_path %>